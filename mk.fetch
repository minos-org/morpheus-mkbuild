fetch:QV: fetch-git fetch-http

fetch-git:QV:
	if test x"$git" != x""; then
		if test x"$branch" = x""; then
			branch=master
		fi
		test -d $src || git clone --depth 1 -b $branch $git $src
	fi

fetch-http:QV:
	if test x"$url" != x""; then
		if ! test -f `basename $url`; then
			wget -c $url
		fi
		tar -xf `basename $url`
	fi
